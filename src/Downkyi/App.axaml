<Application
    x:Class="Downkyi.App"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converter="using:Downkyi.Converter"
    xmlns:local="using:Downkyi"
    RequestedThemeVariant="Light">
    <Application.DataTemplates>
        <local:ViewLocator />
    </Application.DataTemplates>

    <Application.Styles>
        <FluentTheme />

        <!--<StyleInclude Source="/Themes/Default/Styles.axaml" />-->
    </Application.Styles>

    <Application.Resources>
        <!--<ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceInclude Source="/Images/Navigation.axaml" />
                <ResourceInclude Source="/Themes/Default/Theme.axaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>-->

        <!--<converter:BitmapAssetValueConverter x:Key="variableImage" />-->
        <converter:IntEquealConverter x:Key="intEquealConverter" />
        <converter:StringToResourceConverter x:Key="stringToResource" />
    </Application.Resources>
</Application>
